<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyecto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".OwnerProfile"
                  android:label="@string/owner_profile"
                  android:theme="@style/AppTheme"></activity>
        <activity android:name=".OwnerRegister"
                  android:label="@string/owner_register"
                  android:theme="@style/AppTheme"></activity>
        <activity android:name=".RegistrarMascota"
                  android:label="@string/pet_register"
                  android:theme="@style/AppTheme"></activity>
        <activity android:name=".HomeDueno"
                  android:label="@string/home_dueno"></activity>
        <activity android:name=".Register"
                  android:label="@string/register"
                  android:theme="@style/AppTheme"></activity>
        <activity android:name=".Home"
                  android:label="Home"
                  android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>