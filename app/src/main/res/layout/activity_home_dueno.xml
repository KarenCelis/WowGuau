<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeDueno">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#00FFFFFF"
        tools:layout_editor_absoluteX="248dp"
        tools:layout_editor_absoluteY="333dp">

        <RelativeLayout
            android:id="@+id/relative_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="30dp"
            android:paddingRight="30dp">

            <ImageButton
                android:id="@+id/btnBuscar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="0dp"
                android:layout_toStartOf="@+id/btnCalificar"
                android:src="@drawable/search_icon" />

            <ImageButton
                android:id="@+id/btnEnVivo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="0dp"
                android:layout_toLeftOf="@id/btnBuscar"
                android:layout_toEndOf="@+id/btnBuscar"
                android:src="@drawable/live_ride_icon" />

            <ImageButton
                android:id="@+id/btnHistory"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/btnBuscar"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="0dp"
                android:layout_toStartOf="@+id/btnPets"
                android:src="@drawable/history_ride_icon" />

            <ImageButton
                android:id="@+id/btnCalificar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/btnEnVivo"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="0dp"
                android:layout_toLeftOf="@id/btnHistory"
                android:layout_toEndOf="@+id/btnHistory"
                android:src="@drawable/live_ride_icon" />

            <ImageButton
                android:id="@+id/btnProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/btnHistory"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="0dp"
                android:src="@drawable/profile_icon" />

            <ImageButton
                android:id="@+id/btnPets"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/btnCalificar"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="0dp"
                android:layout_toLeftOf="@id/btnProfile"
                android:layout_toEndOf="@+id/btnProfile"
                android:src="@drawable/pethome_icon" />

            <Button
                android:id="@+id/btnnextPet"
                android:layout_width="152dp"
                android:layout_height="52dp"
                android:layout_marginTop="30dp"
                android:text="@string/ajustes"
                android:layout_below="@id/btnProfile"
                android:layout_centerInParent="true"
                android:textColor="@color/blanco"
                android:background="@color/colorVerdeFondoPantallas"/>

        </RelativeLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>