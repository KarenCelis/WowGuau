<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".PPrincipalCliente">

    <Button
        android:id="@+id/ListaPaseadores"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ListaPaseadores" />

    <Button
        android:id="@+id/RegistrarMascota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RegistrarMascota" />

    <Button
        android:id="@+id/ListaMascotas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ListaMascotas" />

    <Button
        android:id="@+id/CerrarSesion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CerrarSesion" />
</LinearLayout>